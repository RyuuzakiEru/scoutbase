    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.0.0",
    "graphql": "^14.4.2",
    "graphql-cli": "^3.0.14",
    "graphql-yoga": "^1.18.1",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^1.19.1",
    "prisma": "^1.34.5",
    "prisma-binding": "^2.3.15"
